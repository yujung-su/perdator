{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
  "strictNullChecks": true, // add if using `base` template
  "allowJs": true, // required, and included with all Astro templates
  "noImplicitAny": false,
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@styles/*": ["src/styles/*"],
      "@blog/*": ["src/blog/*"],
      "@images/*": ["src/images/*"],
      "@scripts/*": ["src/scripts/*"],
      "@assets/*": ["src/assets/*"],
      "@assets2/*": ["./src/assets/*"],
      "@videos/*": ["./src/videos/*"]
    }
  }
}
